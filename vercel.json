{
    "version": 2,
    "builds": [
      {
        "src": "index.html",
        "use": "@vercel/static"
      },
      {
        "src": "styles/*",
        "use": "@vercel/static"
      },
      {
        "src": "scripts/*",
        "use": "@vercel/static"
      }
    ],
    "rewrites": [
        {"source": "/(.*)", "destination": "/Login"}
    ]
  }